{
	"name": "DefaultLinks",
	"version": "1.3.0",
	"author": [
		"foxlit"
	],
	"descriptionmsg": "defaultlinks-description",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"DefaultLinks": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"DefaultLinksMagic": "DefaultLinks.i18n.magic.php"
	},
	"AutoloadNamespaces": {
		"DefaultLinks\\": "src"
	},
	"HookHandlers": {
		"main": {
			"class": "DefaultLinks\\Hooks",
			"services": ["MainConfig", "DBLoadBalancer", "TitleFormatter", "MagicWordFactory"]
		}
	},
	"Hooks": {
		"ParserFirstCallInit": "main",
		"InternalParseBeforeLinks": "main",
		"ParserBeforeInternalParse": "main",
		"RevisionDataUpdates": "main"
	},
	"config": {
		"PagePropLinkInvalidations": {
			"value": {
				"defaultlink": "pagelinks",
				"defaultlinksec": "pagelinks"
			}
		},
		"DFEnabledNamespaces": {
			"value": []
		}
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.29.0"
	}
}
